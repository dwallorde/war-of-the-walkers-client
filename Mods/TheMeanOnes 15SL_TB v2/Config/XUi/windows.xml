<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<set xpath="/windows/window[@name='windowToolbelt']/@pos">-552,92</set>
	<set xpath="/windows/window[@name='windowToolbelt']">
		<rect>
			<sprite depth="8" name="backgroundMain" sprite="menu_empty3px" color="[darkGrey]" type="sliced" fillcenter="false" on_press="true" gamepad_selectable="false"/>
			<rect pos="3,-3" controller="Toolbelt">
				<grid depth="10" name="inventory" rows="1" cols="15" pos="0,0" cell_width="75" cell_height="75" repeat_content="true">
					<item_stack name="0"/>
				</grid>
				<button depth="3" name="btnClearInventory1" sprite="ui_game_symbol_trash" tooltip_key="lblClearInventory" pos="1162,-32" style="icon32px, press, hover" pivot="center" sound="[paging_click]" visible="{creativewindowopen}"/>
				<grid depth="10" name="inventory2" rows="1" cols="30" pos="0,82" cell_width="75" cell_height="75" repeat_content="true" visible="{secondrow}">
					<item_stack name="0"/>
				</grid>
				<button depth="3" name="btnClearInventory2" sprite="ui_game_symbol_trash" tooltip_key="lblClearInventory" pos="1012,50" style="icon32px, press, hover" pivot="center" sound="[paging_click]" visible="{# creativewindowopen and secondrow}"/>
			</rect>
			<grid depth="20" name="numberLabels" rows="1" cols="15" pos="9,-5" cell_width="75" cell_height="20" repeat_content="false">
				<label name="label1" text="1" alignment="left" font_size="19" color="[white]"/>
