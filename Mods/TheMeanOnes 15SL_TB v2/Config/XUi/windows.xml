			<sprite depth="9" pos="0,6" height="8" width="1128" color="[darkGrey]" sprite="menu_empty2px" globalopacity="false" type="sliced" fillcenter="false"/>
			<sprite depth="4" pos="0,6" height="8" width="1128" color="0,255,127,200" globalopacity="false" type="filled" fill="{xp}"/>
			<rect pos="0,-77" width="564" height="8" controller="HUDStatBar" stat_type="Food" visible="{statvisible}">
				<sprite depth="1" color="[mediumGrey]" sprite="menu_empty2px" globalopacity="false" type="sliced"/>
				<sprite depth="9" color="[darkGrey]" sprite="menu_empty2px" globalopacity="false" type="sliced" fillcenter="false"/>
				<filledsprite depth="5" pos="1,0" width="564" name="BarContent" color="0,255,0,200" globalopacity="false" type="filled" fill="0"/>
			</rect>
			<rect pos="563,-77" width="563" height="8" controller="HUDStatBar" stat_type="Water" visible="{statvisible}">
				<sprite depth="1" color="[mediumGrey]" sprite="menu_empty2px" globalopacity="false" type="sliced"/>
				<sprite depth="9" color="[darkGrey]" sprite="menu_empty2px" globalopacity="false" type="sliced" fillcenter="false"/>
				<filledsprite depth="5" pos="1,0" width="563" name="BarContent" color="0,191,255,200" globalopacity="false" type="filled" fill="0"/>
