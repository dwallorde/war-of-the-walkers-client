				<label name="label9" text="9" alignment="left" font_size="19" color="[white]"/>
				<label name="label10" text="10" alignment="left" font_size="19" color="[white]"/>
				<label name="label11" text="11" alignment="left" font_size="19" color="[white]"/>
				<label name="label12" text="12" alignment="left" font_size="19" color="[white]"/>
				<label name="label13" text="13" alignment="left" font_size="19" color="[white]"/>
